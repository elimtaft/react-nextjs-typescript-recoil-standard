    "@prisma/client": "^4.2.1",
    "@testing-library/react": "^13.3.0",
    "@types/formidable": "^2.0.5",
    "axios": "1.2.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "5.2.3",
    "classnames": "^2.3.1",
    "cookies": "^0.8.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "formidable": "^2.0.1",
    "iron-session": "6.3.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "next": "13.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "recoil": "^0.7.6",
    "sass": "^1.54.4",
    "swr": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
