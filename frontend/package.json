{
  "name": "next.js-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "depcheck": "npx depcheck --ignores='eslint,eslint-*,babel-*,@babel*' --skip-missing=true",
    "dev": "next dev",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "format": "prettier --write ./src",
    "generate:migrate:dev": "npm run prisma:generate && npm run prisma:migrate deploy && npm run dev",
    "generate:migrate:prod": "npm run prisma:generate && npm run prisma:migrate deploy && npm run start",
    "jscpd": "jscpd src/",
    "lint": "next lint ./src",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:new:migration": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate",
    "prisma:studio": "npx prisma studio",
    "start": "next start",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.118.1",
